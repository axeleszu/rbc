<?xml version="1.0" encoding="UTF-8"?>
<plan>
    <desayuno>
        <alimento> 1 taza de jugo de naranja con zanahoria </alimento>
        <alimento> 1 Litro agua con una cda clorofila y limón. </alimento>
        <alimento> ACTIVIDAD FÍSICA 30 minutos </alimento>
    </desayuno>
    <almuerzo>
        <alimento> (Mollete sencillo) </alimento>
        <alimento> ½ pieza de bolillo </alimento>
        <alimento> ½ taza de frijol cocido </alimento>
        <alimento> 2 pulgares de queso Oaxaca </alimento>
        <alimento> ½ taza jitomate, limón y cebolla </alimento>
        <alimento> 1 taza agua de Jamaica </alimento>
        <alimento> sin azúcar </alimento>
    </almuerzo>
    <comida>
        <alimento> (Ensalada de atún) </alimento>
        <alimento> 1 taza de fideo </alimento>
